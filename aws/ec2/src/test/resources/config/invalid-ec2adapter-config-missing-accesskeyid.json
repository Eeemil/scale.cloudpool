{
    "scalingGroup": {
      "name": "ec2-cluster",
      "config": {

        "awsSecretAccessKey": "XYZ",
        "region": "us-west-1"
      }
    },
    "scaleUpConfig": {
      "size": "m1.small",
      "image": "ami-018c9568",
      "keyPair": "instancekey",
      "securityGroups": ["webserver"],
      "bootScript": [
        "#!/bin/bash",
        "sudo apt-get update -qy",
        "sudo apt-get install -qy apache2"
      ]
    },
    "scaleDownConfig": {
      "victimSelectionPolicy": "CLOSEST_TO_INSTANCE_HOUR",
      "instanceHourMargin": 0
    },
    "poolUpdatePeriod": 60            
}  
